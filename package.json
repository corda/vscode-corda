{
	"name": "cordalogviewer",
	"displayName": "CordaLogViewer",
	"description": "A log viewer for Corda Nodes",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cordalogviewer.showAllEntries",
		"onCommand:cordalogviewer.showLogViewer",
		"onCommand:cordalogviewer.listenToChanges"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cordalogviewer.showAllEntries",
				"title": "Corda - Show all entries from file"
			},
			{
				"command": "cordalogviewer.showLogViewer",
				"title": "Corda - Show Log Viewer (React)"
			},
			{
				"command": "cordalogviewer.listenToChanges",
				"title": "Corda - Listen to changes in file"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "npm-run-all compile:*",
		"compile:extension": "tsc -p ./",
		"compile:views": "webpack --mode development",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.9.5",
		"@babel/preset-react": "^7.9.4",
		"@types/find": "^0.2.1",
		"@types/node": "^10.12.21",
		"@types/react-dom": "^16.9.8",
		"@types/vscode": "^1.36.0",
		"babel-loader": "^8.1.0",
		"css-loader": "^3.5.2",
		"npm-run-all": "^4.1.3",
		"style-loader": "^1.1.3",
		"ts-import-plugin": "^1.5.4",
		"ts-loader": "^8.0.2",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"url-loader": "^4.1.0",
		"webpack": "^4.15.1",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@material-ui/core": "^4.8.2",
		"@material-ui/icons": "^4.9.1",
		"@types/react": "^16.9.44",
		"axios": "^0.19.1",
		"axios-retry": "^3.1.2",
		"find": "^0.3.0",
		"lodash.merge": "^4.6.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-redux": "^7.1.3",
		"react-redux-toastr": "^7.6.4",
		"react-super-responsive-table": "^5.1.1",
		"redux": "^4.0.4",
		"redux-devtools-extension": "^2.13.8",
		"redux-thunk": "^2.3.0",
		"responsive-table": "^0.1.2",
		"simple-flexbox": "^2.3.2"
	}
}
